{
  "name": "mentorai",
  "version": "2.0.0",
  "description": "AI-powered Discord learning bot with gamification",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "deploy-commands": "node src/deploy-commands.js",
    "admin": "node src/web/server.js",
    "dev:all": "npm run dev",
    "demo": "node scripts/judgingDemo.js",
    "emergency": "node scripts/emergencyFixes.js showStatus",
    "health": "node scripts/emergencyFixes.js healthCheck",
    "test": "node --test tests/**/*.test.js",
    "test:unit": "node --test tests/unit/**/*.test.js",
    "test:e2e": "node --test tests/e2e/**/*.test.js",
    "test:bench": "node tests/benchmarks/performance.test.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@napi-rs/canvas": "^0.1.88",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "discord.js": "^14.18.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "mongoose": "^8.15.1",
    "node-cache": "^5.1.2",
    "openai": "^4.96.2",
    "pdfkit": "^0.14.0",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  }
}
